: java; gutter: false; theme: Confluence" data-theme=3D"Confluence">async f=
unction monitorPositions(walletAddress: string) {
  sdk.setAccount(walletAddress);
 =20
  // Fetch market context
  const { marketsInfoData, tokensData } =3D await sdk.markets.getMarketsInf=
o();
 =20
  // Get all positions
  const positions =3D await sdk.positions.getPositions({
    marketsInfoData,
    tokensData,
    start: 0,
    end: 100
  });
 =20
  // Process positions
  for (const position of positions) {
    console.log(`Position: ${position.market}`);
    console.log(`Size: ${position.size}`);
    console.log(`PnL: ${position.unrealizedPnl}`);
  }
}
</pre>
</div>
</div>
