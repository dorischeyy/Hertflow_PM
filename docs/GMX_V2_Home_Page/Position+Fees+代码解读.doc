<div class=3D"code panel pdl" style=3D"border-width: 1px;">
<div class=3D"codeContent panelContent pdl">
<pre class=3D"syntaxhighlighter-pre" data-syntaxhighlighter-params=3D"brush=
: c; gutter: false; theme: Confluence" data-theme=3D"Confluence">// =E7=BB=
=99=E5=88=B0=E6=B1=A0=E5=AD=90=E7=9A=84=E8=B4=B9=E7=94=A8
fees.feeAmountForPool =3D
    fees.positionFeeAmountForPool +
    fees.borrowing.borrowingFeeAmount -
    fees.borrowing.borrowingFeeAmountForFeeReceiver +
    fees.liquidation.liquidationFeeAmount -
    fees.liquidation.liquidationFeeAmountForFeeReceiver;

// =E6=8A=BD=E6=B0=B4
fees.feeReceiverAmount +=3D
    fees.borrowing.borrowingFeeAmountForFeeReceiver +
    fees.liquidation.liquidationFeeAmountForFeeReceiver;

// =E8=B5=84=E9=87=91=E8=B4=B9=E7=BB=99=E5=AF=B9=E6=89=8B=E6=96=B9
fees.funding =3D getFundingFees(
    fees.funding,
    params.position
);

// ui =E8=B4=B9=E7=BB=99=E6=8F=90=E4=BE=9B=E5=95=86
fees.ui =3D getUiFees(
    params.dataStore,
    params.collateralTokenPrice,
    params.sizeDeltaUsd,
    params.uiFeeReceiver
);</pre>
