            maxPrice,
            tokenOracleType,
            report.signatures[i], // =E5=B8=A6=E4=B8=8A=E6=9D=A5=E7=9A=84=
=E4=BB=B7=E6=A0=BC=E5=BF=85=E9=A1=BB=E6=98=AF=E7=AD=BE=E5=90=8D=E4=BB=B7=E6=
=A0=BC
            signers[i]            // =E9=85=8D=E7=BD=AE=E7=9A=84 signer
        );
    }
       =20
    // ...

    return OracleUtils.ValidatedPrice({
        token: token,
        min: medianMinPrice,
        max: medianMaxPrice,
        timestamp: report.oracleTimestamp,
        provider: address(this)
    });
}</pre>
