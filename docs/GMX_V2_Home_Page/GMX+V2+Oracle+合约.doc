    for (uint256 i =3D 0; i &lt; signers.length; i++) {
        uint256 minPrice =3D report.minPrices[i];
        uint256 maxPrice =3D report.maxPrices[i];

        if (minPrice &gt; maxPrice) {
            revert Errors.InvalidGmSignerMinMaxPrice(minPrice, maxPrice);
        }
        // =E8=BF=99=E9=87=8C=E5=81=9A=E4=BA=86=E9=AA=8C=E7=AD=BE
        GmOracleUtils.validateSigner(
            salt,
