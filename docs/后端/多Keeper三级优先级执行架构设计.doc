func (e *Executor) checkAndExecuteLiquidations(ctx context.Context) {
    // ... =E8=8E=B7=E5=8F=96=E4=BB=B7=E6=A0=BC=E5=92=8C=E5=8F=AF=E6=B8=85=
=E7=AE=97positions ...

    for _, pos :=3D range positions {
        if e.PriorityEnabled {
            if e.PriorityExecutor.ShouldExecuteImmediately() {
                // Master=E6=AD=A3=E5=B8=B8=E6=A8=A1=E5=BC=8F=EF=BC=9A=E7=
=AB=8B=E5=8D=B3=E6=89=A7=E8=A1=8C
                e.executeLiquidation(ctx, &amp;pos, priceData)
            } else {
                // Master=E6=81=A2=E5=A4=8D=E6=A8=A1=E5=BC=8F=E6=88=96Backu=
p=EF=BC=9A=E5=8A=A0=E5=85=A5=E9=98=9F=E5=88=97
                e.PriorityExecutor.jobQueue.Add(pos.PositionKey, JobTypeLiq=
uidation, &amp;pos)
            }
        } else {
            // =E5=8D=95keeper=E6=A8=A1=E5=BC=8F
            e.executeLiquidation(ctx, &amp;pos, priceData)
        }
    }

    // =E5=A4=84=E7=90=86=E9=98=9F=E5=88=97=E4=B8=AD=E5=B7=B2=E5=88=B0=E6=
=9C=9F=E7=9A=84jobs
